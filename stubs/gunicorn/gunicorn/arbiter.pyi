import socket
from types import FrameType
from typing import Type

from gunicorn.app.base import BaseApplication
from gunicorn.config import Config
from gunicorn.glogging import Logger
from gunicorn.workers.base import Worker

from ._types import _AddressType
from .pidfile import Pidfile


class Arbiter:
    WORKER_BOOT_ERROR: int
    APP_LOAD_ERROR: int
    START_CTX: dict[int | str, str | list[str]]
    LISTENERS: list[socket.socket]
    WORKERS: dict[int, Worker]
    PIPE: list[int]
    SIG_QUEUE: list[int]
    SIGNALS: list[int]
    SIG_NAMES: dict[int, str]
    log: Logger | None
    pidfile: Pidfile | None
    systemd: bool
    worker_age: int
    reexec_pid: int
    master_pid: int
    master_name: str
    pid: int
    app: BaseApplication
    cfg: Config
    worker_class: Type[Worker]
    address: list[_AddressType]
    timeout: int
    proc_name: str
    num_workers: int

    def __init__(self, app: BaseApplication) -> None: ...
    def setup(self, app: BaseApplication) -> None: ...
    def start(self) -> None: ...
    def init_signals(self) -> None: ...
    def signal(self, sig: int, frame: FrameType | None) -> None: ...
    def run(self) -> None: ...
    def handle_chld(self, sig: int, frame: FrameType | None) -> None: ...
    def handle_hup(self) -> None: ...
    def handle_term(self) -> None: ...
